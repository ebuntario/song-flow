# Auth.js
AUTH_SECRET=generate-with-npx-auth-secret

# TikTok OAuth (from developers.tiktok.com)
AUTH_TIKTOK_ID=your_tiktok_client_key
AUTH_TIKTOK_SECRET=your_tiktok_client_secret

# Spotify OAuth (from developer.spotify.com)
AUTH_SPOTIFY_ID=your_spotify_client_id
